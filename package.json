{
  "name": "jlgl-pages",
  "displayName": "jlgl-pages",
  "description": "",
  "version": "1.0.0",
  "publisher": "CygraW",
  "repository": "https://github.com/Cygra/jlgl-pages",
  "icon": "https://imgtu.com/i/6VPUoR",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jlgl.pages",
    "onCommand:jlgl.pagesWithRoute",
    "onCommand:jlgl.openInBrowser"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jlgl.pages",
        "title": "JLGL: New JLGL Page Directory"
      },
      {
        "command": "jlgl.pagesWithRoute",
        "title": "JLGL: New JLGL Multi-Page Directory With Route"
      },
      {
        "command": "jlgl.openInBrowser",
        "title": "JLGL: Open Current Folder In Browser"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "jlgl.openInBrowser"
        }
      ],
      "commandPalette": [
        {
          "when": "false",
          "command": "jlgl.openInBrowser"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
